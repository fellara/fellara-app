{
  "expo": {
    "name": "fellas",
    "slug": "fellas",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.jpg",
    "scheme": "fellas",
    "splash": {
      "image": "./src/assets/images/logo.jpg",
      "resizeMode": "contain",
